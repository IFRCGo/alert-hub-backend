[run]
branch = true
source = .
parallel = true
concurrency = multiprocessing
omit = */migrations/*
        */management/*
        */snaphosts/*
        sitecustomize.py
        __init.py__
        */wsgi.py
        manage.py
        .tox/*
        **/apps.py
        **/urls.py
        **/tests.py
        **/test_*.py

[report]
show_missing = true
exclude_lines =
    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
